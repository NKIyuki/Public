<h1>会員情報編集</h1>
<%= form_with model: @custome,url: admin_customer_path(@custome.id) do |f| %>
<h1>会員ID</h1>
<%= @custome.id %>
<h1>氏名</h1>
<%= f.text_field :first_name %>  <%= f.text_field :last_name %>
<h1>フリガナ</h1>
<%= f.text_field :first_name_kana %>  <%= f.text_field :last_name_kana %>
<h1>郵便番号</h1>
<%= f.text_field :postal_code %>
<h1>住所</h1>
<%= f.text_field :address %>
<h1>電話番号</h1>
<%= f.text_field :telephone_number %>
<h1>メールアドレス</h1>
<%= f.text_field :email %>
<%= f.submit '編集内容を保存する',class:"btn btn-success" %>

